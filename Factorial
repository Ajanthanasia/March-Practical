public class Q3 
{
    static int fact(int n)
    {
        if(n == 1 || n == 0)
        {
            return 1;
        }
        else
        {
            return n*fact(n-1);
        }
    }
    public static void main(String as[])
    {
        int num = 5;
        int factNo=fact(num);
        System.out.println("The "+num+" th Factorial number is : "+factNo);

    }
    
}
